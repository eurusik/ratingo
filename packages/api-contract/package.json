{
  "name": "@ratingo/api-contract",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./openapi.json": "./openapi.json"
  },
  "scripts": {
    "fetch:schema": "node ./scripts/fetch-openapi.mjs",
    "generate:types": "openapi-typescript ./openapi.json -o ./src/api-types.ts",
    "update": "npm run fetch:schema && npm run generate:types"
  },
  "devDependencies": {
    "openapi-typescript": "^7.9.1"
  }
}
